<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/aop 
           http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
           http://www.springframework.org/schema/tx 
           http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">
	<bean id="ResultDao" class="exam.Dao.Impl.ResultDaoImpl">
		<property name="hibernateTemplate">
  			<ref bean="hibernateTemplate"/>
  		</property>
	</bean>
	<bean id="ResultService" class="exam.Service.Impl.ResultServiceImpl">
		<property name="commonDao">
			<ref bean="ResultDao" />
		</property>
		<property name="resultdao">
			<ref bean="ResultDao"/>
		</property>
	</bean>
	<bean id="ResultAction" class="exam.action.ResultAction">
		<property name="resultservice">
			<ref bean="ResultService"/>
		</property>	
		<property name="staffservice">
			<ref bean="StaffService"/>
		</property>	
		<property name="paperservice">
			<ref bean="PaperService"/>
		</property>
		<property name="subjectservice">
			<ref bean="SubjectService"/>
		</property>
	</bean>
</beans>